{
    "name": "wiley",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:dev": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "docker-build": "./node_modules/\\@vue/cli-service/bin/vue-cli-service.js build",
        "lint": "vue-cli-service lint",
        "test": "vue-cli-service test:unit",
        "cypress:open": "cypress open",
        "dev": "concurrently \"cd server && npm run start\" \"npm run serve\" ",
        "deploy:dev": "firebase use dev && npm run build:dev && firebase deploy",
        "deploy:prod": "firebase use default && npm run build:prod && firebase deploy"
    },
    "dependencies": {
        "@babel/polyfill": "^7.0.0-rc.1",
        "axios": "^0.19.0",
        "core-js": "^2.6.5",
        "date-fns": "^1.30.1",
        "firebase": "^6.0.1",
        "jsmpeg": "^1.0.0",
        "velocity-animate": "^1.5.2",
        "video.js": "^7.5.4",
        "videojs-rotatezoom": "0.0.1",
        "vue": "^2.6.10",
        "vue-notification": "^1.3.16",
        "vue-router": "^3.0.3",
        "vue-slider-component": "^3.0.31",
        "vuetify": "^1.5.5",
        "vuex": "^3.0.1",
        "vuex-persist": "^2.0.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.7.0",
        "@vue/cli-plugin-eslint": "^3.7.0",
        "@vue/cli-plugin-unit-jest": "^3.8.0",
        "@vue/cli-service": "^3.7.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "concurrently": "^4.1.0",
        "cypress": "^3.3.1",
        "eslint": "^5.16.0",
        "eslint-plugin-cypress": "^2.2.1",
        "eslint-plugin-vue": "^5.0.0",
        "ignore-loader": "^0.1.2",
        "jest-transform-stub": "^2.0.0",
        "lint-staged": "^8.1.5",
        "node-sass": "^4.9.0",
        "sass-loader": "^7.1.0",
        "vue-cli-plugin-vuetify": "^0.5.0",
        "vue-template-compiler": "^2.5.21"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    }
}
